.setup_popup .setup_txt .setup_content {display: flex; justify-content: space-evenly; margin: 15px 10px;}
.setup_popup .setup_txt .setup_content .head {background: #255BB6; color: #fff; width: 35%; white-space: nowrap; padding: 5px; border-radius: 20px;}
