.setup_popup .btn_setup {display: inline-block;}
.setup_popup .btn_setup label > input {display: none;}
.setup_popup .btn_setup label > input:checked + i {background: #4cd9ae;}
.setup_popup .btn_setup label > input:checked + i:after {transform: translateX(20px);}
.setup_popup .btn_setup i {display: inline-block; padding: 2px; width: 40px; height: 20px; border-radius: 13px; vertical-align: middle; transition: .25s .09s; position: relative; background: #d8d9db; box-sizing: initial;}
.setup_popup .btn_setup i:after {content: ''; display: block; width: 20px; height: 20px; border-radius: 50%; background: #fff; position: absolute; left: 2px; transition: .25s;}

