@charset "utf-8";

/*margin*/
.mt_20 {margin-top:20px;}
.mt_50 {margin-top:50px;}
.mb_15 {margin-bottom:15px;}

/*버튼색상*/
.btn_blue {border:none; color:#fff;
background: #499bea; /* Old browsers */
background: -moz-linear-gradient(top,   #6db8ff 0%,#0275ff 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#6db8ff), color-stop(100%,#0275ff)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #6db8ff 0%,#0275ff 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #6db8ff 0%,#0275ff 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #6db8ff 0%,#0275ff 100%); /* IE10+ */
background: linear-gradient(to bottom,  #6db8ff 0%,#0275ff 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#499bea', endColorstr='#207ce5',GradientType=0 ); /* IE6-9 */

}
.btn_black	{border:none; color:#fff;
background: #45484d; /* Old browsers */
background: -moz-linear-gradient(top,  #45484d 0%, #000000 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#45484d), color-stop(100%,#000000)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #45484d 0%,#000000 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #45484d 0%,#000000 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #45484d 0%,#000000 100%); /* IE10+ */
background: linear-gradient(to bottom,  #45484d 0%,#000000 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#45484d', endColorstr='#000000',GradientType=0 ); /* IE6-9 */
}

.btn_green	{border:1px solid #299A0B; color:#fff;
background: #299a0b; /* Old browsers */
background: -moz-linear-gradient(top,  #299a0b 0%, #299a0b 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#299a0b), color-stop(100%,#299a0b)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #299a0b 0%,#299a0b 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #299a0b 0%,#299a0b 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #299a0b 0%,#299a0b 100%); /* IE10+ */
background: linear-gradient(to bottom,  #299a0b 0%,#299a0b 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#299a0b', endColorstr='#299a0b',GradientType=0 ); /* IE6-9 */
}

.btn_gray	{
	border: none;
	background: #999;
	color: #fff;
}

.btn_darkgreen {
	border: none;
	background: #00a1e9;
	color: #fff;
}

.btn_skyblue {
	background: #179ad1;
	border: none;
	color: #fff;
}

.btn_white {
	background: #fff;
	border: 1px solid #179ad1;
	color: #000;
}

.btn_darkblue {
	background: #0f4c95;
	border: none;
	color: #fff;
}

/*버튼*/
.btn_round {padding:5px 10px; border-radius: 5px; font-weight:500; font-size:0.95em;border: none; background-color: #0f4c95; }
.btn_default {padding: 3px 10px; border-radius:20px; font-size:0.95em; letter-spacing: -2px;}
.btn_normal{background-color: #fff; border: 1px solid #0f4c95; box-sizing:border-box; color: #0f4c95; padding: 4px 7px;} 
.btn_mem_check{min-width: 24%;}

.red {color:#ff0000;}
.black {color:#555;}

/* 윤리강령 */
.con_tit{font-size: 1.2rem; text-align: center; margin: 2vh 0; }
.con_tit span{position: relative; display: inline-block;}
.con_tit span::after{content:''; position: absolute; background: #fad5e8;width: 100%; height: 50%; left: 50%;
    transform: translateX(-50%);bottom: 0;z-index: -1;}
.greet_ul li{margin-bottom: 15px; font-size: 0.88rem;}
.indent_en{width: 93%; position: relative; text-indent: -17px; left: 18px;}

/* 한국라이온스 */
.address_txt{font-size: 0.93rem; letter-spacing: -1.7px; font-weight: 700; margin-bottom: 10px;  }
.address img{float: left; width: 40%; margin-right: 8px;border: 1px solid #Eee; height: fit-content;}
.address ul li{margin-bottom: 10px; font-size: 0.9rem;}
.member_1 li{font-size: 0.9rem; margin-bottom: 3px !important; letter-spacing: -1.5px !important;}
.member_tit{font-size: 1rem !important; font-weight: 600; margin-top: 12px; background-color: #eaeaea; padding: 5px 12px;}
.list li{font-size: 0.89rem; letter-spacing: -2px; margin-bottom: 5px !important; /* list-style: inside; text-indent: -19px; */}
.ad2 img{width: 100%;}
.history_wrap{text-align: center;}
.sub_t1{font-size: 1.4rem; font-weight: 700; margin-bottom: 10px;}

/* 테이블 */
.tg_2 {border-collapse:collapse;border-spacing:0; width: 94%; margin: 0 auto;}
.tg_2 td{border-color:#cacaca;border-style:solid;border-width:1px;font-size:13px;
  overflow:hidden;padding:6px 5px;word-break:normal;letter-spacing: -1px;}
.tg_2 th{border-color:#cacaca;border-style:solid;border-width:1px;font-size:13px;
  font-weight:normal;overflow:hidden;padding:3px 5px;word-break:normal;letter-spacing: -2px;}
.tg_2 .tg-uqo3{background-color:#E0DFEF;text-align:center;}
.tg_2 .tg-yj5y{background-color:#E0DFEF;border-color:#cacaca;text-align:center;}
.tg_2 .tg-baqh{text-align:center;}
.tg_2 .tg-c3ow{border-color:#cacaca;text-align:center;}
.tg_2 .tg-amwm{font-weight:500;text-align:center; color: #2f2f2f;}
.tg_2 .tg-0lax{text-align:left;}


/* 지구총재 */

.tg{width: 90%; margin: 0 auto;}
.tg td{border-color:#b9b9b9;border-style:solid;border-width:1px;font-size:14px;
  overflow:hidden;padding:2px 6px;word-break:normal;}
.tg th{border-color:#b9b9b9;border-style:solid;border-width:1px;font-size:14px;
  font-weight:normal;overflow:hidden;padding:2px 3px;word-break:normal;}
.tg .dex{width: 28%; text-align: center;background-color: #f1f3f3;}
.tg .ans{padding-left: 8px;}

/* 지구현황 */
.global_box{margin: 0 17px;}
.global_box dl dt{font-weight: 500; letter-spacing: -1px; font-size: 0.9em;}
.global_box dl dd{margin: 5px 10px; font-size: 0.88em;}
.global_box dl .tt{font-weight: 500;}





/* 실천과제 */
.mark_text ul li{margin-bottom: 16px; font-size: 1.25rem;}

