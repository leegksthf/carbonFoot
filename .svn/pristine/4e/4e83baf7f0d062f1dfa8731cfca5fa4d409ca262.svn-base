.gallary_author{margin-top: .25rem;font-size: 14px;color: #969696;}

.gallery_list li {position:relative; height:90px; margin-bottom:10px; padding:7px 7px 7px 150px; border:1px solid #d8d8d8; background:#fff; font-size:0.9em; vertical-align:top;}
.gallery_list img {position:absolute; top:7px; left:7px; width:135px; height:90px;}
.gallery_list dt {margin:5px 0; font-size:1.2em; font-weight:500; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.gallery_list dd {padding-right:5px; font-size:0.9em; line-height:130%;}
.gallery_list .year {margin-top:5px; color:#999; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}

.paging {text-align:center; font-size:1.1em;}
.paging strong {font-weight:500; color:#ff0000;}
.paging span img {height:23px;}

.admin_paging {position:relative; padding:5px 0; margin: 0 auto; font-size:1.1em; text-align: center;}
.admin_paging ul{display: flex; *display:inline;*zoom:1; margin:0 auto; text-align:center;justify-content: center; align-items: center;}
.admin_paging li {float:left; margin: 0 5px;}
.admin_paging a, .admin_paging a:link, .admin_paging a:visited, .admin_paging a:active {display:inline-block; font-weight:500; vertical-align: middle;}
.admin_paging a:hover, .admin_paging .paging_on a {background:transparent; color:#fff;}
.admin_paging li {background: #255BB6; border-radius: 50%; width: 25px; height: 25px; text-align: center; line-height: 20px;}
.admin_paging li a{font-weight: 300 !important; color: #d0e1ff;}
.admin_paging .paging_on {background: #003a7d; border-radius: 50%; width: 25px; height: 25px; text-align: center; line-height: 20px;}
.admin_paging .paging_img{background-color: #fff;}
.admin_paging .paging_img a{padding:0; background:transparent; color: #999;}
 
.board_view {margin: 15px 0; background:#fff; }
.board_view .board_date{font-size: 13px;}
#container .board_view h3 {margin:0; padding:0; font-weight: 500; color:#000; background:none; font-size: 16px; padding-bottom: 5px;}
#container .board_view h3 input {margin:0;}
#container .board_view input {width:88%; margin:5px 0; border-radius:10px; padding:10px 5%;}
.board_view p {color: #999; font-size: .95rem;}
.board_view p.gallery_date{padding-top: 5px;}
.board_view .b_file {border: none; padding: 3px 0; display: flex;}
.board_view .b_file a{color: #999999; font-size: .9rem; margin: 0 4px;text-overflow: ellipsis; overflow: hidden;}
.board_view .b_file i {color: #999999;}
/* .board_view .b_file i {margin-right: 4px; color: #999999;} */
.board_view .year {color:#000;}
.board_view .view_content {padding:10px 0; border-top: 1px solid #eee; border-bottom: 1px solid #eee; margin: 5px 0;}
.board_view .view_content .event_con_txt{color: #000; font-weight: 300; word-break: keep-all;}
.board_view .view_content .event_img{border: 1px solid #eeeeee;}
.board_view .view_content textarea {width:88%;padding:10px 5%; border-radius:10px; min-height:150px;}
.board_view .view_content img {max-width:100%; display: block;}

.board_view .event_date_tb{font-size: 14px; width: 100%; margin-top: 10px; }
.board_view .event_date_tb tr{border-bottom: 1px solid #dcdcdc; }
.board_view .event_date_tb tr:first-child{border-top: 1px solid #dcdcdc;}
.board_view .event_date_tb th{vertical-align: top; text-align: left;background: #eee; padding: 5px; }
.board_view .event_date_tb td{padding: 5px; padding-right: 0; word-break: keep-all;}
.btn_box{display: flex; justify-content: space-between; align-items: center;}

/* 게시판 글작성 */
.board_write select {border: 1px solid #ccc; border-radius: 20px; padding: 0 5px;}
.board_write .btn {margin-top:10px; padding:10px; text-align:center;}
.board_write table {width:100%;}
.board_write tbody {border-top: 2px solid #255BB6; border-bottom: 2px solid #255BB6;}
.board_write th, .board_write td {background: transparent; padding:7px; border: 0; border-bottom: 1px solid #e2e2e2;}
.board_write td {font-size: .85em; white-space: nowrap;}
.board_write th{/* background:#eee; */ font-size: 14px; letter-spacing: -2px; color: #666; white-space: nowrap;}
.board_write td input[type~="text"]{ height: 28px; border: none;} 
.board_write td textarea{width:100%; resize: none;  border: none;}
.board_write td select {border-radius: 0;}
.board_write td dt{margin:5px 0 10px 0; color:#999; padding-left:20px; background:url(../images/icon_name.gif) left top no-repeat;}
.board_write td dt strong {font-size:14px; color:#000;}
.board_write table .title {text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.board_write td ul li{float: left;margin-right: 10px;}
.board_write td div{margin-top: 10px;}
.board_write td div p{margin-top: 5px;}
.board_write td > li > input[type="text"] {border: 1px solid #eaeaea; margin: 2px 0; box-sizing: border-box;}
.board_write input[id="cb"] {display: none;}
.board_write input[id="cb"] + label {
	display: inline-block;
	width: 20px;
	height: 20px;
	border: 1px solid #ccc;
	cursor: pointer;
	border-radius: 4px;
	vertical-align: middle;
} 
.board_write input[id="cb"]:checked + label {
	background: #74CACB;
	position: relative;
	border: 1px solid #74CACB;
	border-radius: 4px;
}

.board_write input[id="cb"]:checked + label::before {
	content: '✔';
	color: #fff;
	width: 20px;
	height: 20px;
	position: absolute;
	display: inline-block;
	text-align: center;
	font-size: 16px;
	line-height: 20px;
}
.board_write #boardAddress {position: relative; width: 90%;}
.board_write .search_btn {width: 30px; height: 30px; text-align: center; color: #fff; background: #255bb6; border: none; ertical-align: middle;}
