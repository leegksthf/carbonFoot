.biz_like .like_l .heart_icon img{width: 55%; position: absolute; left: 50%; top: 50%; transform: translate(-50%, -50%);}
.biz_like .like_l p{display: inline-block; vertical-align: middle; font-size: 14px; color: #7f7f7f;}
.biz_like .like_l .heart_icon i{position: absolute; left: 50%; top: 50%; transform: translate(-50%, -50%); color: #fff; font-size: 15px;}
.biz_like .like_r{float: right; font-size: 15px; padding: 0 5px;position: relative;height: 30px; display: flex; align-items: center; justify-content: center;}
.biz_like .like_r .three_dot_wrap{display: flex; height: 100%; width: 100%; justify-content: center; align-items: center;}
.biz_like .like_r .three_dot{display: flex;}
.biz_like .like_r .three_dot div{width: 4px; height: 4px; background-color: #7f7f7f; border-radius: 100%;}
.biz_like .like_r .three_dot div:nth-child(2){margin: 0 3px;}

.biz_like .like_r ul{position: absolute; background: #fff; border: 1px solid #ccc; box-sizing: border-box;text-align: center;width: 70px; right: 0; top: 25px; box-shadow: 0 3px 5px #00000038; display: none;}
.biz_like .like_r ul.edit_show_list{display: block;}
/*.biz_like .like_r.edit_show ul{display: block;}*/
.biz_like .like_r ul li{width: 100%;font-size: 14px;}
.biz_like .like_r ul li:first-child{border-bottom: 1px solid #ccc;}
.biz_like .like_r ul li a{padding: 3px 0;display: block;}

.biz_size_icon{position: absolute; right: 5px; bottom: 5px;}
.biz_size_icon span{width: 25px;  height: 25px; display: flex; text-align: center; justify-content: center; align-items: center; background: #00000091; border-radius: 5px; color: #fff;}

.biz_like .like_l.on .heart_icon{background: url(/images/heart_on.svg) no-repeat center 100%;}
.biz_like .like_l.on p{color: #e82e8a;}

.biz_com .biz_txt{background-color: #e2e2e2; border-radius: 50px; padding: 0 15px;height: 30px; line-height: 30px;display: flex;
    align-items: center;}
.biz_com .biz_txt::after{content: ''; display: block; clear: both;}
.biz_com .biz_txt input{box-sizing: border-box; float: left;border: 0;width: 90%; background-color: transparent; height: 30px; line-height: 30px; font-size: 14px;}
.biz_com .biz_txt button{box-sizing: border-box; float: right; width: 10%;border: 0; background-color: transparent; font-size: 16px; height: 30px; color: #777777; align-items: center;
    justify-content: center;}
.biz_com_list{width: 100%; padding: 0px 5px 10px; box-sizing: border-box;}
/*.biz_com_list li:last-child{border: 0;}*/
.biz_com_list .biz_com_top{color: #0f4c95;font-weight: 400; font-size: 14px; padding-bottom: 3px; }
.biz_com_list .biz_com_btm{font-size: 14px; color: #000000;display: flex; align-items: center; justify-content: space-between; font-weight: 300;}
.biz_com_list .biz_com_btm p{white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.biz_com_list .biz_com_btm span{    min-width: 50px; color: #7f7f7f; text-align: right; font-size: 13px;}

/*비즈홈 서브화면 상단바*/
.biz_sub_t{ background-color: #fff;   position: fixed; z-index: 100; width: 100%; border-bottom: 1px solid #ececec; }
.biz_sub_t > a{ display: block; padding: 8px 3vw; position: absolute; left: 0; top: 0;}
.biz_sub_t > p{padding: 10px 3vw; text-align: center;font-size: 15px; font-weight: 500; color: #4c4c4c;}

/*비즈홈 댓글*/
.biz_comment_con { padding-bottom: 55px;}
.biz_comment_con .biz_com_list .biz_com_btm > p { white-space: normal; overflow: revert; text-overflow: revert; word-break: keep-all;}
.biz_comment_con .biz_com_list{padding: 0 5px;}
.biz_comment_con .biz_com_top, .biz_comment_con .biz_com_btm{font-size: 15px;}

.biz_comment_con .biz_com_list li{padding: 10px; border-bottom: 1px solid #ececec;}
.biz_comment_con .biz_com_time{font-size: 13px; color: #777777; font-weight: 300; padding-top: 5px;}

.biz_comment_con .biz_com{padding: 10px;}

/*비스홈 수정*/
.biz_change .biz_box{padding: 20px 15px 80px;}
.biz_change .biz_top .biz_top_txt{padding: 5px 0 10px;width: 85vw; margin: auto;}
.biz_change .biz_top .biz_top_txt textarea{width: 100%;margin: 0; box-sizing: border-box; resize: none; border: 1px solid #eeeeee; outline: none; display: block; padding: 5px;}
.biz_change .biz_top .biz_top_txt #txt_conut{ text-align: right; font-size: 14px; color: #a1a1a1;}

/*비스홈 작성*/
.biz_write .biz_box{padding: 20px 15px 80px;}
.biz_write .biz_top .biz_top_txt{padding: 5px 0 10px;width: 85vw; margin: auto;}
.biz_write .biz_top .biz_top_txt textarea{width: 100%;margin: 0; box-sizing: border-box; resize: none; border: 1px solid #eeeeee; outline: none; display: block; padding: 5px;}
.biz_write .biz_top .biz_top_txt #txt_conut{ text-align: right; font-size: 14px; color: #a1a1a1;}
.biz_write .biz_img{font-size: 15vw; color: #e82e8a;}
.biz_write .biz_image{position: relative;}
.biz_write .biz_image i{position: absolute;right: -3vw;  bottom: 0; font-size: 7vw; color: #0f4c95;}
.biz_write .biz_image i:before{background: #fff; border-radius: 100%;}

/*회원사 찾기*/
.excutive_con{padding-bottom: 35px;}
.excutive_con::after{content: ''; display: block; clear: both;}
.excutive_con .search_wrap_view{ float: left; margin-right: 10px;padding: 0;}
.excutive_con .search_wrap_view .search_field{width: 100%;}
.excutive_con .my_location{float: right;line-height: 35px; margin-top: 1vh; text-align: right; color: #e82e8a; width: 80px; flex-grow: 1;display: flex;
    justify-content: space-between; align-items: center;}


.excutive_con .my_location span{font-size: 14px; }
.excutive_con .my_location.rotate i{-webkit-animation: rotate 1s linear; animation: rotate 1s linear; }
@keyframes rotate{
	0%{ transform: rotate(0deg);}
	100%{transform: rotate(540deg);}
}

.excutive_con .excutive_top{display: flex; justify-content: space-between; align-items: center; padding: 0 1vh;}

/*커뮤니티*/
.community_con{padding-bottom: 10vh; overflow: hidden;}
.community_con .zone_con .vote_tit_r{white-space: nowrap; max-width: none;}
.community_con .zone_con ol::after{content: ''; display: block; clear: both;}
.community_con .zone_con ol li{font-size: 15px;float: left; width: 25%; text-align: center;}
.community_con .vote_value{width: 20vw; height: 20vw; border-radius: 100%; margin: auto; color: #171717; background-color: #fff; position: relative;}
.community_con .vote_value01{background-color: #595959; color: #fff;}
.community_con .vote_value > div{position: absolute; left: 50%; top: 50%; transform: translate(-50%, -50%);  width: 100%; font-size: 14px;}
.community_con .vote_zone_wrap {margin-bottom: 2vh;}
.community_con .vote_zone_wrap .zone_con li:first-child{padding-bottom: 2vw;}
.community_con .vote_zone_wrap .zone_con > li > div{display: flex; font-size: 14px;}
.community_con .vote_zone_wrap .zone_con > li > div::after{content: ''; display: block; clear: both;}
.community_con .vote_zone_wrap .zone_con > li > div span{float: left;}
.community_con .vote_zone_wrap .zone_con > li > div span.vote_tit_l{padding-right: 10px; white-space: nowrap;}
.community_con .vote_zone_wrap .zone_con > li > div span.vote_tit_r{flex-grow: 1; text-overflow: ellipsis; overflow: hidden;}



/*클럽검색*/
.staff_area {text-align: center; padding: 0 1%; padding-bottom: 1%;}
.staff_area > div{display: none; justify-content: space-between;box-sizing: border-box;border-radius: 10px; overflow: hidden;border: 1px solid #dcdcdc;}
.staff_area .staff_img{text-align: center;}
.staff_area .staff_img img{height: 22vw; width: auto;}
.staff_area > div > div > p{padding: 5px; background: #eee; color: #000; font-size: 15px; font-size: 14px; font-weight: 500; line-height: 110%;}
.staff_area .staff_pro_l{width: 50%; border-right: 1px solid #dcdcdc;}
.staff_area .staff_pro_r{width: 50%;}
.staff_area .staff_pro_area{display: flex;}
.staff_area .staff_img{display: flex;}
.staff_area .staff_txt{display: flex; flex-direction: column; justify-content: center; align-items: center; padding: 0 2px; flex-grow: 1; }
.staff_area .staff_txt li{font-size: 5vw;}
.staff_area .staff_txt li:first-child{font-size: 4vw; word-break: keep-all; white-space: nowrap;}
.staff_area .staff_txt li span{font-size: 4vw; padding-right: 1vw;}

/*커뮤니티 행사참여*/

.request_tb th{width:117px; background:#eee; font-size: 14px;  white-space: nowrap; letter-spacing: -1px; padding: 5px 0;}
.request_search_btn{color: #fff;font-weight: 400; padding: 2px 10px !important;}
.request_con .board_search{padding: 5px 0;}
