html {height:100%;}
body {margin:0; padding:0; height:auto; background: #fff; color:#000; font-family: 'Noto Sans KR', sans-serif;}
div {margin:0; padding:0; font-size:1em; font-family: 'Noto Sans KR', sans-serif; -webkit-text-size-adjust:none; box-sizing: border-box;}
div img{width: 100%;}

input, textarea, select {margin:0; padding:0; -webkit-appearance: none; -webkit-border-radius: 0; appearance: none; font-family: 'Noto Sans KR', sans-serif;}
form {display:block; border:none;}
fieldset {margin:0px; padding:0px; border:none;}
input {padding:0px; vertical-align:middle; border:none; outline: none;}
input[type~="text"] {border:1px solid #ccc; padding:1px;}
input[type~="password"] {border:1px solid #eaeaea; padding:1px;}
input[type~="file"] {border:1px solid #eaeaea; padding:2px 0 2px 5px;}
input[type~="checkbox"] {border:none; margin:-1px 0; padding:2px 0 2px 5px;}
input[type~="radio"] {border:none; margin:-1px 0; padding:2px 0 2px 5px;}
input[type~="button"]{padding-top:3px; cursor:pointer;}
input[type~="submit"]{padding-top:3px; cursor:pointer;}
input[type~="image"]{margin:0; padding:0; cursor:pointer;}
.wrap-loading{z-index: 1000000;}

select {
	vertical-align:middle; 
	padding:0; 
	background: url(https://farm1.staticflickr.com/379/19928272501_4ef877c265_t.jpg) no-repeat 95% 50%;
	background-size: 20%;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	outline: none;
}
textarea {padding:2px; border:1px solid #ccc;}

a {margin:0; padding:0; color:#333; -webkit-text-size-adjust:none;}

p {margin:0; padding:0; line-height:130%;}

strong {margin:0; padding:0; font-weight: bold; }
Form {display:inline; margin:0; padding:0;}
img {border:none; margin:0; padding:0;}
fieldset {margin:0; padding:0; border:0;}
table {margin:0;  border-collapse:collapse; table-layout:fixed;}
td{word-break:break-all;font-weight: 300;}
th{word-break:break-all; font-weight: 500;}

ul,li,ol,dl,dd,dt {margin:0; padding:0; list-style:none; overflow:hidden;}
hr {padding:0; margin:0;}

h1 {margin:0; padding:0; -webkit-text-size-adjust:none;}
h2 {margin:0; padding:0; -webkit-text-size-adjust:none;}
h3 {margin:0; padding:0; -webkit-text-size-adjust:none;}
h4 {margin:0; padding:0; -webkit-text-size-adjust:none;}
h5 {margin:0; padding:0; -webkit-text-size-adjust:none;}

a:link   {text-decoration:none; border:none;}
a:visited{text-decoration:none; border:none;}
a:hover	 {text-decoration:none; border:none;}
a:active {text-decoration:none; border:none;}
a        {text-decoration:none; border:none;}
a:focus	 {border:none;}

.clear{clear:both; width:100%;}
.float_l{float:left;}
.float_r{float:right;}

.align_c    {text-align:center !important;}
.align_r    {text-align:right !important;}
.align_l    {text-align:left !important;}

.valign_t{vertical-align:top;}
.valign_m{vertical-align:middle;}
.valign_b{vertical-align:bottom;}

.strong{font-weight:bold;}
.f_normal{ font-weight:normal !important;}
.nobg{background:none !important;}
.noline{border:none !important;}

.nomargin{margin:0 !important;}
.nopadding{padding:0 !important;}

.hand{cursor:pointer;}
.hide{display: none;}
