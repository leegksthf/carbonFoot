		<script type="text/javascript">
			$(document).ready(function(){
				var menu = "<%=menuNum%>";
				$('#lnb ul li').eq(menu).addClass('on');
			});
			
			function MM_openBrWindow(theURL,winName,features,number,mode) {
				if(number != '')
					theURL += '?mode=' + mode + '&number=' + number;
				else
					theURL += '?mode=' + mode;
				window.open(theURL,winName,features);
			}
			function winOpen(){
				var win = window.open('/', 'index','width=540,height=900,scrollbars=yes');
				win.focus();
			}
			function winOpen2(){
				var win = window.open('/member_list?century=2020', 'member_list','width=540,height=900,scrollbars=yes');
				win.focus();
			}		
		</script>
